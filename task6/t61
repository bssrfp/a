import java.sql.*;
public class MetaDetaEx {

	public static void main(String[] args) {
		try {
			Class.forName("com.mysql.cj.jdbc.Driver");
			Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/college","root","admin");
			DatabaseMetaData dbmd=con.getMetaData();
			System.out.println("Product:"+dbmd.getDatabaseProductName());
			System.out.println("Version:"+dbmd.getDatabaseProductVersion());
			System.out.println("Driver Name:"+dbmd.getDriverName());
			System.out.println("User:"+dbmd.getUserName());
			Statement st=con.createStatement();
			ResultSet rs=st.executeQuery("select * from students");
			ResultSetMetaData rsmd=rs.getMetaData();
			for(int i=1;i<=rsmd.getColumnCount();i++) {
				System.out.println("Coulumn name:"+rsmd.getColumnName(i));
				System.out.println("Type:"+rsmd.getColumnTypeName(i));
				
			}
			
		}
		catch(Exception e) {
			System.out.println(e.getMessage());
		}
	}
}
